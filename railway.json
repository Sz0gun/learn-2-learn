{
    "build": {
      "builder": "NIXPACKS",
      "nixpacksConfig": {
        "install": [
          "pip install -r django_fst/requirements.txt"
        ]
      }
    },
    "deploy": {
      "services": {
        "django": {
          "startCommand": "python django_fst/manage.py runserver 0.0.0.0:$PORT",
          "envVars": {
            "DJANGO_SETTINGS_MODULE": "core.settings",
            "PORT": "8000"
          }
        }
      }
    }
  }
  